program ZAD_B_2;

{$APPTYPE CONSOLE}

uses
  SysUtils;

var
   a:array [1..26] of integer;
   i,j,n,k,x:integer;
   s:int64;
   st:string;
begin
   readln(n,k);
   readln(st);
   for i:=1 to 26 do a[i]:=0;
   for i:=1 to n do inc(a[ord(st[i])-64]);
   for i:=2 to 26 do
      begin
         x:=a[i];
         for j:=i-1 downto 1 do
            begin
               if (a[j]<x) then a[j+1]:=a[j]
               else begin a[j+1]:=x; break; end;
               if (j=1) then a[1]:=x;
            end;
      end;
   s:=0;
   for i:=1 to 26 do
      begin
         if (k>=a[i]) then
            begin
               s:=s+trunc(1.0*a[i]*a[i]);
               k:=k-a[i];
            end
         else
            begin
               s:=s+trunc(1.0*k*k);
               break;
            end;
      end;
   writeln(s);
   readln;
end.
 